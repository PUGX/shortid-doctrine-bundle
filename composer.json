{
    "name": "pugx/shortid-doctrine-bundle",
    "description": "Symfony bundle for ShortId Doctrine type",
    "keywords": ["symfony", "bundle", "shortid", "doctrine"],
    "homepage": "http://pugx.org/",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        },
        {
            "name": "The Community",
            "homepage": "https://github.com/pugx/shortid-doctrine-bundle"
        }
    ],
    "support": {
        "issues": "https://github.com/pugx/shortid-doctrine-bundle/issues"
    },
    "require": {
        "pugx/shortid-doctrine": "^0.3",
        "symfony/config": "^2.8|^3.4|^4.0",
        "symfony/dependency-injection": "^2.8|^3.4|^4.0",
        "symfony/http-kernel": "^2.8|^3.4|^4.0"
    },
    "require-dev":{
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": { "PUGX\\ShortidDoctrineBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "PUGX\\ShortidDoctrineBundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
