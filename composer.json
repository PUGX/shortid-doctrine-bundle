{
    "name": "pugx/shortid-doctrine-bundle",
    "description": "Symfony bundle for ShortId Doctrine type",
    "keywords": ["symfony", "bundle", "shortid", "doctrine"],
    "homepage": "http://pugx.org/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        },
        {
            "name": "The Community",
            "homepage": "https://github.com/pugx/shortid-doctrine-bundle"
        }
    ],
    "support": {
        "issues": "https://github.com/pugx/shortid-doctrine-bundle/issues"
    },
    "require": {
        "symfony/symfony": "~2.7",
        "pugx/shortid-doctrine": "1.*@dev"
    },
    "require-dev":{
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": { "PUGX\\ShortidDoctrineBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    }
}
